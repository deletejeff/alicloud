<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.grgbanking.alicloud.dao.user.UserDao">
    <update id="addUserPoints">
        update user set points = points + #{points} where userid = #{userid}
    </update>
</mapper>